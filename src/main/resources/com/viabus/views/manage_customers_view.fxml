<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.viabus.controllers.ManageCustomersController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
               <children>
                  <TableView fx:id="customerTableView" prefHeight="347.0" prefWidth="600.0">
                     <columns>
                        <TableColumn fx:id="customerIdColumn" prefWidth="74.0" text="ID " />
                        <TableColumn fx:id="customerFirstNameColumn" minWidth="0.0" prefWidth="127.0" text="First Name" />
                        <TableColumn fx:id="customerLastNameColumn" minWidth="0.0" prefWidth="133.0" text="Last Name" />
                        <TableColumn fx:id="customerPhoneColumn" prefWidth="123.0" text="Phone" />
                        <TableColumn fx:id="customerEmailColumn" prefWidth="141.0" text="Email" />
                     </columns>
                  </TableView>
                  <Button layoutX="72.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleAddCustomerButton" prefHeight="26.0" prefWidth="105.0" text="Add Customer" />
                  <Button layoutX="248.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleEditCustomerButton" prefHeight="26.0" prefWidth="105.0" text="Edit" />
                  <Button layoutX="433.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleDeleteCustomerButton" prefHeight="26.0" prefWidth="105.0" text="Delete" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
